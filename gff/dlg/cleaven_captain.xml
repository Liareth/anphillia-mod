            <CExoLocString Name="Text">
                <DWord Name="StringRef">4294967295</DWord>
                <SubString>
                    <Int Name="StringID">0</Int>
                    <CExoString Name="String">Well, there are many tasks at hand.  I'll give you the choice.  There's the goblin problem, the need for equipment, beetlebellies, or.. well.. hmm..</CExoString>
                </SubString>
            </CExoLocString>
        </Struct>
        <Struct Id="5">
            <Byte Name="AnimLoop">1</Byte>
            <DWord Name="Animation">0</DWord>
